document.addEventListener('DOMContentLoaded', function () {
    var main = new Splide('#main-carousel', {

        type: 'fade',
        // padding: '5rem',
        autoplay: true,
        wheel: true,
        arrows: false,
        start: 1,



    });

    var thumbnails = new Splide('#thumbnail-carousel', {
        fixedWidth: 200,
        fixedHeight: 50,
        gap: 10,
        rewind: true,
        pagination: false,
        autoplay: true,

        focus: 1.5,
        isNavigation: true,
        start: 1,
        // arrows: false,

        breakpoints: {
            600: {
                fixedWidth: 60,
                fixedHeight: 44,
            },
        },
    });

    main.sync(thumbnails);
    main.mount();
    thumbnails.mount();
});

