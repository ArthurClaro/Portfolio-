$(document).ready(function () {
    $('#myCanvas').tagcanvas({
        textColour: 'rgb(255, 255, 255)',
        outlineThickness: 0.5,
        outlineColour: '#fe0853',
        maxSpeed: 0.06,
        freezeActive: true,
        shuffleTags: true,
        shape: 'sphere',
        zoom: 1.1,
        noSelect: true,
        textFont: null,
        pinchZoom: true,
        freezeDecel: true,
        fadeIn: 3000,
        initial: [0.3, -0.1],
        depth: 0.8
    });
})

