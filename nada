let h = document.querySelector("#primeiro")

function entrada() {

    h.classList.add(`animate__rubberBand`)
    document.querySelector(".blast").removeAttribute("id");
}
function saida() {


    setTimeout(() => {
        h.classList.remove(`animate__rubberBand`)
    }, "500");

}
h.addEventListener("mouseover", entrada)
h.addEventListener("mouseout", saida)

